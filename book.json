{
  "gitbook": "3.2.2",
  "title": "python3学习笔记",
  "description": "我的python 3学习笔记",
  "language": "zh",
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "anchors",
    "todo",
    "github",
    "edit-link",
    "tbfed-pagefooter",
    "search-plus",
    "prism",
    "prism-themes",
    "book-summary-scroll-position-saver"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/hysg/python3"
    },
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy hysg 2017",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "edit-link": {
        "base": "https://github.com/hysg/python3/edit/master",
        "label": "编辑本页"
    },
    "prism": {
        "css": [
        "prism-themes/themes/prism-atom-dark.css"
        ]
    }
  }
}
