{
  "gitbook": "3.2.2",
  "title": "python3学习笔记",
  "description": "我的python 3学习笔记",
  "author": "hysg",
  "language": "zh-hans",
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "anchors",
    "todo",
    "mermaid",
    "github",
    "github-buttons",
    "edit-link",
    "tbfed-pagefooter",
    "search-plus",
    "prism",
    "prism-themes",
    "book-summary-scroll-position-saver"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/hysg/python3"
    },
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy hysg 2017",
        "modify_label": "本页修订时间(UTC+8)：",
        "modify_format": "YYYY-MM-DD"
    },
    "edit-link": {
        "base": "https://github.com/hysg/python3/edit/master",
        "label": "编辑本页"
    },
    "prism": {
        "css": [
        "prism-themes/themes/prism-atom-dark.css"
        ]
    },
    "github-buttons": {
        "repo": "hysg/python3",
        "types": [
                "star",
                "watch",
                "fork"
         ],
        "size": "small"
   }
  }
}
